<% title "Tweet" %>

<p>
  <strong>Source:</strong>
  <%=h @tweet.source_id %>
</p>
<p>
  <strong>Text:</strong>
  <%=h @tweet.text %>
</p>

<p>
  <%= link_to "Edit", edit_tweet_path(@tweet) %> |
  <%= link_to "Destroy", @tweet, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", tweets_path %>
</p>
